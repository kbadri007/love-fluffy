    "boilerplate",
    "browser-sync",
    "clean-css",
    "gulp",
    "imagemin",
    "sass",
    "webpack"
  ],
  "main": "gulpfile.js",
  "scripts": {
    "start": "gulp",
    "build": "set NODE_ENV=production gulp",
